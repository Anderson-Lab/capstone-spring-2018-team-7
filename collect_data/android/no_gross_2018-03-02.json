    summary: 'Welcome to Animal Crossing: Pocket Camp!',
    icon: '//lh3.googleusercontent.com/AmLXdyZWfl749UkmVMknEsKHHBh_zBqDEpl4P36Pg8_0idEmYhymkyw8MRORkLqhnzbL=w300',
