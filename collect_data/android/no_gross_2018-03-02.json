    developer: 'Playrion',
    developerId: '6457893425161276771',
    developerEmail: 'marion@airlines-manager.com',
    developerWebsite: 'http://www.airlines-manager.com',
    developerAddress: '141 avenue de Wagram, 75017 Paris',
    updated: 'February 23, 2018',
