  { title: 'War of Colony',
    summary: 'The 1707-1920 colonial war of the world',
    icon: '//lh3.googleusercontent.com/WP_3v4GihAVHO1FaWywMPauGRcjVSvu7UhPDj-IEkhaFSGS4xXv-v6H68MUoErBBqw=w300',
    price: '0',
