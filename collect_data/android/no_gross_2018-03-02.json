    score: 4.6,
    reviews: 67522,
    developer: 'Nodding Frog Ltd',
    developerId: 'Nodding+Frog+Ltd',
    developerEmail: 'support@ironmaidenlegacy.com',
    developerWebsite: 'http://ironmaidenlegacy.com/',
    developerAddress: '10 John Street\nLondon, London WC1N 2EB\nUnited Kingdom',
