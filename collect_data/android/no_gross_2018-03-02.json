    maxInstalls: 50000000,
    score: 4.5,
    reviews: 1419108,
    developer: 'ELECTRONIC ARTS',
    developerId: '6605125519975771237',
    developerEmail: 'help@eamobile.com',
    developerWebsite: 'http://help.ea.com/en/madden/madden-nfl-mobile/',
