    score: 4.4,
    reviews: 570510,
    developer: 'Hothead Games',
    developerId: 'Hothead+Games',
    developerEmail: 'support@hotheadgames.com',
    developerWebsite: 'http://www.hotheadgames.com',
    developerAddress: '1555 West Pender Street\nVancouver, BC V6G 2T1\nCanada',
