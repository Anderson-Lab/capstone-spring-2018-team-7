    developerAddress: 'Warner Bros. Entertainment\nWARNER BROS ENTERTAINMENT, INC.\n4000 WARNER BLVD\nLOS ANGELES\nBURBANK\nUnited States\nCalifornia\n91522-0001',
    updated: 'February 28, 2018',
    version: undefined,
