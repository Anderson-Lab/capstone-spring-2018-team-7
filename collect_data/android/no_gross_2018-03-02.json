    summary: 'Story changes depending on your choices.',
    icon: '//lh3.googleusercontent.com/A-YWoG0cj_4Y_cJIOf8-h18W9G-rbXCiZOsN_iXKTD432wpPWSnl_IvV2O0rydJSfFNO=w300',
