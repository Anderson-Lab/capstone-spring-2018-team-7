    developerEmail: 'android.support@zynga.com',
    developerWebsite: 'https://zyngasupport.helpshift.com/a/wizard-of-oz-magic-match/?l=en',
    developerAddress: '699 8th Street, San Francisco. CA',
    updated: 'March 1, 2018',
