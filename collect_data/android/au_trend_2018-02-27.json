    developerEmail: 'xooglegamemario@gmail.com',
    developerWebsite: 'https://web.facebook.com/notes/bastian-steel-developer/bastian-steel-developer/129116250947215',
    developerAddress: 'Suramadu',
    updated: 'February 22, 2018',
