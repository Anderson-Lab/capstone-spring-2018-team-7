    developerEmail: 'support@nekki.mail.helpshift.com',
    developerWebsite: 'http://www.nekki.com',
    developerAddress: '116 Gladstonos Street M. KYPRIANOU HOUSE, 3&4 floor, 3032, Limassol, Cyprus.',
