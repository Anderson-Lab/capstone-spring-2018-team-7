    reviews: 88867,
    developer: 'Mobile Sports Time',
    developerId: '6822219618520028060',
    developerEmail: 'mobilesportstime@gmail.com',
    developerWebsite: 'https://docs.google.com/document/d/1TehjpeZviQomDFMhfZDDjGnKFxpRbIW5k1R7nIh57Qk/edit?usp=sharing',
