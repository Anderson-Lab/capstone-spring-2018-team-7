    summary: 'Like your life - but instant.',
    icon: '//lh3.googleusercontent.com/Gq2F-J6VYXwofOLFI-yU9cnNJMWUJ5_iUlBdl8YPEDDlMJOIGggFGkAt2qm-5xFBGSQ=w300',
