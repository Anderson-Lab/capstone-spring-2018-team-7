    minInstalls: 10000000,
    maxInstalls: 50000000,
    score: 4.6,
    reviews: 466041,
    developer: 'Noodlecake Studios Inc',
    developerId: '9078867188205035581',
    developerEmail: 'support+altos@noodlecake.com',
