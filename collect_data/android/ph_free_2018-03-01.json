    reviews: 235946,
    developer: 'Shiny Box, LLC',
    developerId: 'Shiny+Box,+LLC',
    developerEmail: 'shinyboxllc@gmail.com',
    developerWebsite: 'http://www.dungeon-quest.com',
    developerAddress: 'Po Box 131044 Houston Texas 77219',
    updated: 'December 22, 2017',
