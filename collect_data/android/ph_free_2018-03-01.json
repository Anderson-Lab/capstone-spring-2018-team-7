  { title: 'Big Little Farmer Offline Farm',
    summary: 'Big Little Farmer is a unique blend of farming game first time in offline mode!',
    icon: '//lh3.googleusercontent.com/SfjwJAWJROI72Sfbs9gvqVCjlVQ3nuKuoKhOB_86BRmlRlU7LbK_Yvy1QYSUNmiqxIlT=w300',
    price: '0',
    free: true,
    minInstalls: 5000000,
    maxInstalls: 10000000,
    score: 4.3,
    reviews: 105567,
    developer: 'Innovation Drive Studios',
    developerId: '6923881983932747154',
    developerEmail: 'support.blf@innovationdrivestudios.com',
