    reviews: 257818,
    developer: 'My.com B.V.',
    developerId: '5496590613422638755',
    developerEmail: 'hustle-castle@corp.my.com',
    developerWebsite: 'http://hc.my.com',
    developerAddress: 'Barbara Strozzilaan 201, 1083 HN in Amsterdam',
