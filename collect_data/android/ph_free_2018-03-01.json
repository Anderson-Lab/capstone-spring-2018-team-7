    developerEmail: 'seasnake.198209@gmail.com',
    developerWebsite: 'http://www.feelingtouch.com/feelingtouch_privacy.htm',
    developerAddress: 'RM 1701(228) 17/F HENAN BLDG\nNO 90 JAFFE RD WANCHAI\nHongKong',
