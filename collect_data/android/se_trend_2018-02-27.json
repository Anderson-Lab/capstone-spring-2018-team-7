    developerEmail: 'animalswildlifestudio@gmail.com',
    developerWebsite: 'http://animalswildlifestu.wixsite.com/animalswildlife/single-post/2017/12/19/ANIMALS-WILDLIFE-STUDIO-PRIVACY-POLICY',
    developerAddress: undefined,
    updated: 'February 5, 2018',
