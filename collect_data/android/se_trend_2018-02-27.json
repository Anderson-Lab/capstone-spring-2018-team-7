    summary: 'Save the villagers and the island from the evil creatures!',
    icon: '//lh3.googleusercontent.com/HDFaZtfj2XACrMZFuKpYBHXUfb3t01TEw-JnMGD2OYKXfa2N-jx0MyFrZStR6CqI_g8I=w300',
    price: '0',
    free: true,
    minInstalls: 100000,
    maxInstalls: 500000,
    score: 4.4,
    reviews: 509,
    developer: 'Game Pickle',
    developerId: 'Game+Pickle',
    developerEmail: 'gamepicklestudios@gmail.com',
    developerWebsite: 'http://www.i6.com/mobile-privacy-policy/',
    developerAddress: '2 Railway Terrace,\nSouth Hylton.\nSunderland\nSR4 0PD',
