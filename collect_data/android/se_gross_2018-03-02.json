    developer: 'Bethesda Softworks LLC',
    developerId: 'Bethesda+Softworks+LLC',
    developerEmail: 'falloutshelterhelp@mail.bethesda.net',
    developerWebsite: 'http://fallout.bethsoft.com',
    developerAddress: 'Bethesda Softworks LLC\n1370 Piccard Drive, Suite 120\nRockville, MD 28050',
