    score: 4.1,
    reviews: 1334,
    developer: 'Gro Play Digital',
    developerId: '8132222129320389077',
    developerEmail: 'info@groplay.com',
    developerWebsite: 'http://www.groplay.com',
    developerAddress: 'Gro Play c/o Sup46\nRegeringsgatan 29\n111 53 Stockholm\nSweden',
    updated: 'October 13, 2017',
