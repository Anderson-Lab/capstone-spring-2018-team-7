    summary: 'Endless slingshot challenges.',
    icon: '//lh3.googleusercontent.com/MnnaSc_3_8C3mJeYDuzcnbdrJCtQ1cUzHK_hzsvpUFD6k7dfuUszqyZmDrbpD1bzJYY=w300',
    price: '0',
