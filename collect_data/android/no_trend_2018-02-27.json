  { title: 'Bacon Escape',
    summary: 'Fast paced adventure game with challenging levels and lots of carts &amp; animals',
    icon: '//lh3.googleusercontent.com/8T-jSBTmpJaSQMHgEY_OQbrA58A1pd8MaXM1fKgDz-mzYEKrcA_ygA3EfERhOcwDEQ=w300',
    price: '0',
    free: true,
    minInstalls: 500000,
    maxInstalls: 1000000,
    score: 4.2,
    reviews: 4493,
    developer: 'Illusion Labs',
    developerId: '7495700166596495310',
    developerEmail: 'support@illusionlabs.se',
    developerWebsite: 'http://www.illusionlabs.se',
    developerAddress: 'Illusion Labs AB\nKalendegatan 25\n211 35 Malmo\nSweden',
