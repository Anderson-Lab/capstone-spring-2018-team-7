    maxInstalls: 5000000,
    score: 4.2,
    reviews: 5806,
    developer: 'Disney',
    developerId: 'Disney',
    developerEmail: 'support@disneymobile.com',
    developerWebsite: 'https://help.disney.com/Games/Doc-McStuffins-Baby-Nursery',
    developerAddress: '500 South Buena Vista Street\nBurbank, CA 91521-0105\nUnited States',
    updated: 'November 17, 2017',
    version: '1.2',
