    developerWebsite: 'https://docs.google.com/document/d/1FCnr4-rb5groJn5AyR6nUMgjUDyjgdETInizu1CRM90/edit?usp=sharing',
    developerAddress: 'Leonberg',
    updated: 'February 9, 2018',
