  { title: 'Yoga Challenge App',
    summary: 'Seen the yoga challenge videos online? Want to do the Yoga Challenge? Install.',
    icon: '//lh3.googleusercontent.com/-Pm-CHA7cibI0VAGo87GG6aWbPLUacxwFrGPQJQcdQQdfd69yvAnKNp5G4P5MPu_l4ak=w300',
    price: '0',
