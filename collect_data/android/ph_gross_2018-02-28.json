    reviews: 4595,
    developer: 'CHUANG COOL ENTERTAINMENT',
    developerId: 'CHUANG+COOL+ENTERTAINMENT',
    developerEmail: 'liuguohongck@163.com',
    developerWebsite: 'http://glygj.szckhd.com/index.php?a=home',
    developerAddress: 'UNI 17,9/F TOWER A NEW MANDARIN PLAZA NO.14 SCIENCE MUSEUM RD TST KL',
