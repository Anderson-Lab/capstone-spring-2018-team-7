    score: 4.6,
    reviews: 223443,
    developer: 'FTX Games LTD',
    developerId: 'FTX+Games+LTD',
    developerEmail: 'narcos@ftxgames.com',
    developerWebsite: 'http://www.ftxgames.com',
    developerAddress: 'Petousis House\nStrovolou 146-148\nStrovolos\nNicosia\nCyprus\n2048',
