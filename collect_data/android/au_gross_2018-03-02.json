  { title: 'Narcos: Cartel Wars',
    summary: 'Create the ultimate cartel in the official action game for the TV show Narcos!',
    icon: '//lh3.googleusercontent.com/1lInWbcjTtie8qz4Jbok93QJRBbJjng97duRSBgKh0UScyrjhdM8Ovk4zB_KEz-8ww=w300',
    price: '0',
