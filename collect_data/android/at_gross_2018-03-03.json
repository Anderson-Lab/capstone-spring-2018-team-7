  { title: 'Animal Jam - Play Wild!',
    summary: 'In Play Wild! become a virtual animal, adopt pets, and play fun games!',
    icon: '//lh3.googleusercontent.com/F2R48qDRxdrlfeplSN4qeZU4SycXzEte7IwviNKLyu2MmzylRsO0i3f4lTm51FUjgg=w300',
    price: '0',
