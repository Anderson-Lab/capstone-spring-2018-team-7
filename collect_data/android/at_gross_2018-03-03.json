    score: 4.5,
    reviews: 257226,
    developer: 'Supersolid',
    developerId: 'Supersolid',
    developerEmail: 'support@supersolid.com',
    developerWebsite: 'http://www.supersolid.com',
    developerAddress: 'Supersolid Ltd\n19-20 Berners Street\nLondon\nW1T 3NW',
